    <actuator>
        <motor gear="100" joint="r_hip_yaw_joint" name="r_hip_yaw"/>
        <motor gear="100" joint="r_hip_roll_joint" name="r_hip_roll"/>
        <motor gear="100" joint="r_hip_pitch_joint" name="r_hip_pitch"/>
