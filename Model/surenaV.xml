                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="l_hip_yaw_link" pos="0.016506 0.1 -0.0256">
                <inertial pos="-0.07571 9e-05 -0.09" quat="0.376612 0.597667 0.599307 0.376548" mass="3.1707" diaginertia="0.0193514 0.0168395 0.00710166" />
