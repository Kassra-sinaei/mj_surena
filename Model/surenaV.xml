                        <body name="l_knee_link" pos="0.00103758 0 -0.359883">
                            <inertial pos="0.01575 -0.00162 -0.08673" quat="0.99964 0.00885815 0.00570931 0.0246636" mass="2.23418" diaginertia="0.0319986 0.0308854 0.00571325" />
